{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": { "ecmaVersion": "latest" },
    "rules": {
        "no-unused-vars": "off",
        "no-plusplus": "off",
        "no-param-reassign": ["error", { "props": false }]
    }
}
